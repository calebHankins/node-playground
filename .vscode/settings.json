{
    "emojisense.languages": {
        "markdown": true,
        "git-commit": false,
        "plaintext": false,
        "json": true,
        "javascript": true
    },
    "emojisense.unicodeCompletionsEnabled": true,
    "emojisense.markupCompletionsEnabled": true,
    "emojisense.showOnColon": true,
    "cSpell.words": [
        "appdata",
        "bierner",
        "buildcache",
        "Buildx",
        "davidanson",
        "dbaeumer",
        "devcontainer",
        "eamodio",
        "emojisense",
        "extensionpack",
        "hookyqr",
        "mhutchie",
        "mutantdino",
        "openssh",
        "promisify",
        "quokka",
        "remotehub",
        "resourcemonitor",
        "shwoopsie",
        "sketchbuch",
        "tink",
        "vids",
        "wallabyjs",
        "yzhang"
    ],
    "cSpell.useGitignore": true,
    "explorer.excludeGitIgnore": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.markdownlint": "explicit"
    },
    "markdownlint.config": {
        "MD028": false,
        "MD025": {
            "front_matter_title": ""
        }
    }
}
